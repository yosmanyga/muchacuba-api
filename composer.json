{
    "name": "muchacuba/muchacuba",
    "description": "",
    "authors": [
        {
            "name": "Yosmany Garcia",
            "email": "yosmanyga@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "algolia/algoliasearch-client-php": "~1.10",
        "symsonte/symsonte": "@dev",
        "mongodb/mongodb": "~1",
        "yosmy/facebook-profile": "@dev",
        "yosmy/privilege-profile": "@dev",
        "yosmy/unique": "@dev",
        "google/cloud": "@dev",
        "guzzlehttp/guzzle": "^6.2",
        "kreait/firebase-tokens": "^1.5",
        "mailgun/mailgun-php": "^2.1",
        "php-http/curl-client": "^1.7",
        "respect/validation": "~1",
        "symfony/intl": "^3.3",
        "yosmanyga/validation": "@dev",
        "willdurand/email-reply-parser": "^2.6",
        "mmucklo/email-parse": "^2.0",
        "sentry/sentry": "^1.8"
    },
    "require-dev": {
        "coduo/php-matcher": "~2",
        "behat/behat": "~3",
        "fabpot/goutte": "^3.2",
        "fzaninotto/faker": "~1.6",
        "sebastian/comparator": "^2.0",
        "symfony/debug": "~3",
        "symsonte/behat-extension": "@dev",
        "willdurand/geocoder": "~3.3"
    },
    "autoload": {
        "psr-4": {
            "Muchacuba\\": [
                "setup",
                "src/http",
                "src/upgrade",
                "src/user"
            ],
            "Muchacuba\\Internauta\\": [
                "src/internauta"
            ],
            "Muchacuba\\Http\\": [
                "src/http"
            ],
            "Muchacuba\\Cli\\": [
                "src/cli"
            ],
            "Yosmy\\Navigation\\": [
                "src/yosmy/navigation"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muchacuba\\Internauta\\Test\\": [
                "test/features/internauta"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://php.yosmy.com"
        },
        {
            "type": "composer",
            "url": "https://php.holapana.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
