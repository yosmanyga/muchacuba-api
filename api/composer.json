{
    "name": "cubalider/muchacuba",
    "authors": [
        {
            "name": "Yosmany Garcia",
            "email": "yosmanyga@gmail.com"
        },
        {
            "name": "Manuel Emilio Carpio",
            "email": "mectwork@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symsonte/symsonte": "@dev",
        "algolia/algoliasearch-client-php": "~1.10",
        "cubalider/unique": "@dev",
        "cubalider/authentication-profile": "@dev",
        "cubalider/internet-profile": "@dev",
        "cubalider/privilege-profile": "@dev",
        "facebook/graph-sdk": "~5.3",
        "google/cloud": "^0.20.2",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.3",
        "lcobucci/jwt": "^3.2",
        "mailgun/mailgun-php": "^2.1",
        "monolog/monolog": "^1.22",
        "php-http/curl-client": "^1.7",
        "symsonte/security-asserter": "dev-master",
        "yosmanyga/validation": "@dev"
    },
    "require-dev": {
        "coduo/php-matcher": "~2",
        "behat/behat": "~3",
        "fabpot/goutte": "^3.2",
        "fzaninotto/faker": "~1.6",
        "symfony/debug": "~3",
        "symsonte/behat-extension": "@dev",
        "willdurand/geocoder": "~3.3"
    },
    "autoload": {
        "psr-4": {
            "Muchacuba\\": [
                "domain/setup",
                "domain/src"
            ],
            "Muchacuba\\Http\\": [
                "http/setup",
                "http/src"
            ],
            "Muchacuba\\Cli\\": [
                "cli/setup",
                "cli/src"
            ],
            "Cubalider\\Facebook\\": [
                "domain/vendor/cubalider-facebook"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muchacuba\\": [
                "domain/features"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:yosmanyga/symsonte.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:yosmanyga/symsonte-extension.git"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
